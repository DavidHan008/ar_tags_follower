<launch>
  
  <node pkg="rbx2_ar_tags" name="ar_follower" type="ar_follower.py" clear_params="true" output="screen">
    <rosparam>
       rate: 100
       max_x: 4.0
       goal_x: 0.8
       x_threshold: 0.1
       y_threshold: 0.1
       y_scale: 1.0
       x_scale: 1.0
       max_angular_speed: 0.2
       min_angular_speed: 0.4
       max_linear_speed: 1.5
       min_linear_speed: 0
    </rosparam>
    
  </node>
</launch>
